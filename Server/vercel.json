{
    "version": 2,
    "name":"Server",
    "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods":["POST,GET,PUT,DELETE"],
      "headers": {
        "Access-Control-Allow-Orign":"*"
      }
    }
  ]
}

















    
  